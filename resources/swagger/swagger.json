{
    "swagger": "2.0",
    "basePath": "/api/v0.1",
    "info": {
        "version": "0.1",
        "title": "Video upload"
    },
    "paths": {
        "/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "description": "User ligin",
                "parameters": [
                    {
                        "name": "email",
                        "in": "formData",
                        "description": "User email",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "formData",
                        "description": "User password",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "title": "Rresponse",
                            "type": "object",
                            "properties": {
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "api_token": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/video/upload": {
            "post": {
                "tags": [
                    "Video"
                ],
                "description": "Upload video by user",
                "parameters": [
                    {
                        "name": "video",
                        "in": "formData",
                        "description": "File for upload",
                        "required": true,
                        "type": "file"
                    },
                    {
                        "name": "duration",
                        "in": "formData",
                        "description": "Video duration",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "start_time",
                        "in": "formData",
                        "description": "Where video will be start",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "api_token",
                        "in": "formData",
                        "description": "API token for auth",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "title": "Response with message",
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/video/list": {
            "get": {
                "tags": [
                    "Video"
                ],
                "security": [
                    {
                        "internalApiKey": []
                    }
                ],
                "description": "Get list of video by user",
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "title": "Rresponse",
                            "type": "object",
                            "properties": {
                                "data": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/definitions/Video"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "securityDefinitions": {
        "internalApiKey": {
            "type": "apiKey",
            "in": "header",
            "name": "Authorization"
        }
    },
    "definitions": {
        "Video": {
            "type": "object",
            "properties": {
                "video": {
                    "type": "object",
                    "properties": {
                        "origin": {
                            "type": "string"
                        },
                        "cut": {
                            "type": "string"
                        }
                    }
                },
                "duration": {
                    "type": "integer"
                },
                "status": {
                    "type": "string"
                }
            }
        }
    }
}